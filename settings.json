{
  "symbols.hidesExplorerArrows": false,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, .eslint*, prettier*, tsconfig*, vite*, pnpm-lock*, bun.lockb, nest*",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*",
    "tailwind.config.js": "tailwind.config*, postcss.config*",
    ".env.local": ".env*",
    ".env": ".env*"
  },
  "tailwindCSS.experimental.classRegex": [
    ["tv\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    "class:\\s*?[\"'`]([^\"'`]*).*?,"
  ],
  "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "terminal.integrated.fontFamily": "JetBrains Mono, Consolas, 'Courier New', monospace",

  "explorer.fileNesting.enabled": true,
  "window.restoreFullscreen": true,
  "update.mode": "manual",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "workbench.editor.labelFormat": "short",
  "editor.guides.bracketPairs": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript", "javascriptreact", "graphql"],
  "[prisma]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "workbench.colorTheme": "Falcon - Vue Theme",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "catppuccin-macchiato",
  "dart.flutterSdkPath": "C:\\flutter",
  "glassit.alpha": 220,
  "vscord.status.state.text.editing": "üìù Editing {file_name}{file_extension} in üìÇ {directory_name}",
  "vscord.status.details.text.editing": "üõ†Ô∏è In {workspace} {problems}",
  "vscord.status.problems.text": "- {problems_count} problems",
  "vscord.status.state.text.viewing": "üìù Viewing {file_name}{file_extension} in üìÇ {directory_name}",
  "vscord.app.name": "Visual Studio Code",
  "vscord.status.details.text.idle": "üïí Idle for a while",
  "vscord.status.details.text.notInFile": "üö´ No file open",
  "python.createEnvironment.trigger": "off",
  "dart.flutterCreatePlatforms": ["android", "ios"],
  "breadcrumbs.enabled": false,
  "editor.stickyScroll.enabled": false,
  "[xml]": {
    "editor.defaultFormatter": "Oracle.oracle-java"
  },
  "[java]": {
    "editor.defaultFormatter": "Oracle.oracle-java"
  },
  "workbench.layoutControl.enabled": false,
  "editor.minimap.enabled": false,

  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.scrollbar.horizontalScrollbarSize": 10,
  "editor.scrollbar.vertical": "visible",
  "editor.scrollbar.horizontal": "visible",

  "workbench.colorCustomizations": {
    "editor.background": "#21222f",
    "sideBar.background": "#21222f",
    "activityBar.background": "#21222f",
    "actionBar.toggledBackground": "#21222f",
